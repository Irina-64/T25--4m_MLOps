{
  "id": null,
  "uid": "detox-monitoring",
  "title": "Detox API Monitoring",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Requests per second",
      "id": 1,
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(detox_request_total[1m])) by (endpoint)",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Latency quantiles",
      "id": 2,
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(detox_request_latency_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "p50 {{endpoint}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.9, sum(rate(detox_request_latency_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "p90 {{endpoint}}",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(detox_request_latency_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "p95 {{endpoint}}",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Prediction length",
      "id": 3,
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum(rate(detox_prediction_length_sum[5m])) / sum(rate(detox_prediction_length_count[5m]))",
          "legendFormat": "avg length",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(detox_prediction_length_bucket[5m])) by (le))",
          "legendFormat": "p95 length",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    }
  ]
}
